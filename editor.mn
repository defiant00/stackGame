use ace

Editor
	AceEd
	Warnings: []
	
	Init: fn()
		AceEd: ace.edit('editor')
		AceEd
			setTheme('ace/theme/stack')
			getSession().setMode('ace/mode/stack')

	AddWarning: fn(line, text)
		Warnings.push({row, column, text, type: line, 0, text, 'warning'})

	ClearWarnings: fn()
		Warnings: []

	ShowWarnings: fn()
		AceEd.getSession().setAnnotations(Warnings)
